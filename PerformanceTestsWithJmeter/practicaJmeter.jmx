                <stringProp name="script">List&lt;String&gt; prices = new ArrayList&lt;&gt;();

for (int i=1; i&lt;=(vars.get(&quot;getPrice_matchNr&quot;)as int); i++){
	prices.add((vars.get(&quot;getPrice_&quot;+i)));
}
